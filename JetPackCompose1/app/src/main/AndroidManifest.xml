<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.JetPackCompose1"
        tools:targetApi="31">
        <activity
            android:name=".ui.MusicKnobActivity"
            android:exported="true"
            android:label="@string/title_activity_music_knob"
            android:theme="@style/Theme.JetPackCompose1">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ui.CircularProgressBarActivity"
            android:exported="true"
            android:label="@string/title_activity_animation"
            android:theme="@style/Theme.JetPackCompose1">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ui.AnimationActivity"
            android:exported="true"
            android:label="@string/title_activity_animation"
            android:theme="@style/Theme.JetPackCompose1"></activity>
        <activity
            android:name=".ui.ConstraintLayoutActivity"
            android:exported="true"
            android:label="@string/title_activity_constraint_layout"
            android:theme="@style/Theme.JetPackCompose1"></activity>
        <activity
            android:name=".ui.ListActivity"
            android:exported="true"></activity>
        <activity
            android:name=".ui.SnackBarActivity"
            android:exported="true"
            android:label="@string/title_activity_snack_bar"
            android:theme="@style/Theme.JetPackCompose1"></activity>
        <activity
            android:name=".StateCounterActivity"
            android:exported="true"></activity>
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.JetPackCompose1"></activity>
    </application>

</manifest>